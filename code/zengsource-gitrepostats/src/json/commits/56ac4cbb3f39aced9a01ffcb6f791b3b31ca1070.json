cope.isHidden = !$scope.isHidden;\n++                if(!$scope.isHidden){\n++                    alert($scope.backMess[0].message);\n++                }\n++            });\n++        }\n++});\n++// use the myAppModule variable to\n++// configure the module with a filter\n++myAppModule.filter('stripDashes', function() {\n++    return function(txt) {\n++        // filter code would go here\n++}; }); \n+"
