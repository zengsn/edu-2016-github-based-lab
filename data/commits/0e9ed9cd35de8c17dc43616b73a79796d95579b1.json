    "patch" : "@@ -0,0 +1,52 @@\n+<div class=\"color\">\n+<div class=\"container\">\n+      <div class=\"starter-template\">\n+        <h1>销售信息</h1>    \n+      </div>         \n+</div><!-- /.container -->  \n+\t<form class=\"form-horizontal\" role=\"form\" ng-submit=\"create()\">\n+    <fieldset>\n+        <div class=\"form-group\">\n+\t\t\t <label for=\"saleOrder\"  class=\"col-md-1 control-label\">销售订单：</label>\n+            <div class=\"col-md-2\">\n+                <input class=\"form-control disabled\" id=\"saleOrder\" type=\"text\" />\n+            </div> \n+            <label for=\"orderType\"  class=\"col-md-1 control-label\">订单类型：</label>\n+            <div class=\"col-md-2\">\n+                <select id=\"orderType\" class=\"form-control\" ng-model=\"seleted\" ng-options=\"a.orderType for a in SelectItem\">               \n+                </select>\n+            </div>\n+\t\t\t<label for=\"orderDate\"  class=\"col-md-1 control-label\">起始日期：</label>\n+            <div class=\"col-md-2\">\n+                <input id=\"orderDate\"  type=\"date\"/>\n+            </div>\n+\t\t\t<label for=\"deliveryDate\"  class=\"col-md-1 control-label\">交货日期：</label>\n+            <div class=\"col-md-2\">\n+                <input id=\"deliveryDate\"  type=\"date\"/>\n+            </div>\n+\n+          \n+        </div>\n+\t\t  <div class=\"form-group\">\n+\t\t\t<label for=\"orderNumber\"  class=\"col-md-1 control-label\">订单编号：</label>\n+            <div class=\"col-md-2\">\n+                 <input class=\"form-control \" id=\"orderNumber\" type=\"text\" placeholder=\"只可读不可编辑\" disabled/>\n+            </div>\n+\t\t\t<label for=\"orderState\"  class=\"col-md-1 control-label\">订单状态：</label>\n+            <div class=\"col-md-2\">\n+                 <input class=\"form-control \" id=\"orderState\" type=\"text\" placeholder=\"只可读不可编辑\" disabled/>\n+            </div>\n+\t\t\t<label for=\"remark\"  class=\"col-md-1 control-label\">备注：</label>\n+            <div class=\"col-md-2