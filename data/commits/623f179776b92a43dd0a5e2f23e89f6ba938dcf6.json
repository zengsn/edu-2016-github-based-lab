    "patch" : "@@ -0,0 +1,23 @@\n+body {\n+  font: normal 16px/1.4 Georgia;\n+}\n+input:not([type='checkbox']), select {\n+  width: 250px;\n+}\n+select, input {\n+  padding: 5px;\n+  margin-top: 12px;\n+  font-family: inherit;\n+}\n+input[type='submit'] {\n+  width: 264px;\n+}\n+form span {\n+  color: red;\n+}\n+input[name='email'].ng-dirty.ng-invalid {\n+  color: red;\n+}\n+input[name='email'].ng-dirty.ng-valid {\n+  color: green;\n+}"
